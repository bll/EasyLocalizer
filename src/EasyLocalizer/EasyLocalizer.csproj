
<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Description>Simple localization library via to your localization strategy.</Description>
    <Company>bll</Company>
    <Authors>Bilal YanÄ±k</Authors>
    <PackageLicenseExpression></PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/bll/Localizer</PackageProjectUrl> 
    <RepositoryUrl>https://github.com/bll/Localizer</RepositoryUrl>
    <PackageId>EasyLocalizer</PackageId>
    <PackageIcon>EasyLocalizer.png</PackageIcon>
    <PackageTags>EasyLocalizer</PackageTags>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <AssemblyVersion>1.0.0.0</AssemblyVersion>
    <FileVersion>1.0.0.0</FileVersion>
    <Version>1.0.0</Version>
    <PackageVersion>1.0.0</PackageVersion>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\..\img\EasyLocalizer.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
    <None Include="..\..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>